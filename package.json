{
  "name": "cv-with-nuxt",
  "license": "MIT",
  "scripts": {
    "audit:packages": "yarn install --audit",
    "dev": "nuxt --config-file ./config/nuxt.config.js",
    "build": "nuxt build --config-file ./config/nuxt.config.js",
    "start": "nuxt start --config-file ./config/nuxt.config.js",
    "generate": "nuxt generate --config-file ./config/nuxt.config.js --fail-on-error",
    "test": "jest --config ./config/jest.config.js",
    "test:watch": "yarn test --watch",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "nuxt": "^2.12.2",
    "nuxt-i18n": "^6.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^2.0.5",
    "vue-jest": "^4.0.0-beta.2"
  }
}
