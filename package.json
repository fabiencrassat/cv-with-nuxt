{
  "name": "cv-with-nuxt",
  "license": "MIT",
  "engines": {
    "node": ">=12.15.0"
  },
  "scripts": {
    "audit:packages": "yarn install --audit",
    "build": "nuxt build",
    "clear": "yarn test --clearCache",
    "dev": "yarn outdated & yarn dev:offline",
    "dev:offline": "nuxt",
    "generate": "nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "now-build": "yarn generate",
    "start": "nuxt start",
    "start:static": "yarn http-server -p 3000 ./dist",
    "test": "jest --config ./config/jest.config.js",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "validate:lighthouse": "yarn lhci autorun --config=./config/lighthouserc.js",
    "validate:pages": "node scripts/validate_pages.js"
  },
  "dependencies": {
    "@nuxtjs/robots": "2.4.2",
    "@nuxtjs/sitemap": "2.4.0",
    "@nuxtjs/tailwindcss": "3.4.2",
    "nuxt": "2.14.12",
    "nuxt-i18n": "6.18.0",
    "nuxt-lazy-load": "1.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@lhci/cli": "0.6.1",
    "@nuxt/types": "2.14.12",
    "@nuxt/typescript-build": "2.0.4",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/pwa": "3.3.4",
    "@types/jest": "26.0.20",
    "@vue/test-utils": "1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "dotenv": "8.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.5.0",
    "html-validator": "5.1.17",
    "http-server": "0.12.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-serializer-vue": "2.0.2",
    "levenary": "1.1.1",
    "lint-staged": "10.5.3",
    "mock-fs": "4.13.0",
    "pa11y": "5.3.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.0",
    "vue-jest": "3.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  }
}
