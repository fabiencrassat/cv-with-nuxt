{
  "name": "cv-with-nuxt",
  "license": "MIT",
  "engines": {
    "node": ">=14.19.3 <18"
  },
  "scripts": {
    "audit:packages": "yarn install --audit",
    "build": "nuxt build",
    "clear": "yarn test --clearCache",
    "dev": "yarn outdated & yarn dev:offline",
    "dev:offline": "nuxt",
    "generate": "nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "now-build": "yarn generate",
    "prepare": "husky install",
    "start": "nuxt start",
    "start:static": "yarn http-server -p 3000 ./dist",
    "test": "jest --config ./config/jest.config.js",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "validate:lighthouse": "yarn lhci autorun --config=./config/lighthouserc.js",
    "validate:pages": "node scripts/validate_pages.js"
  },
  "dependencies": {
    "@nuxtjs/robots": "2.5.0",
    "@nuxtjs/sitemap": "2.4.0",
    "@nuxtjs/tailwindcss": "3.4.3",
    "nuxt": "2.14.12",
    "nuxt-i18n": "6.28.1",
    "nuxt-lazy-load": "1.2.9"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@lhci/cli": "0.10.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/pwa": "3.3.5",
    "@types/jest": "26.0.24",
    "@vue/test-utils": "1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "dotenv": "16.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "7.20.0",
    "html-validator": "6.0.1",
    "http-server": "14.1.1",
    "husky": "^8.0.0",
    "jest": "26.6.3",
    "jest-serializer-vue": "3.1.0",
    "levenary": "1.1.1",
    "lint-staged": "13.1.0",
    "mock-fs": "5.2.0",
    "pa11y": "5.3.1",
    "prettier": "2.8.8",
    "ts-jest": "26.5.6",
    "vue-jest": "3.0.7"
  },
  "lint-staged": {
    "*.{js,vue,ts}": "eslint --cache --fix"
  }
}
