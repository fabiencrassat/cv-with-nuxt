{
  "name": "cv-with-nuxt",
  "license": "MIT",
  "engines": {
    "node": ">=20.16.0 <100"
  },
  "type": "module",
  "scripts": {
    "analyze": "nuxt analyze",
    "audit:packages": "yarn install --audit",
    "build": "yarn build:vercel",
    "build:node_server": "nuxt build --preset node_server",
    "build:vercel": "nuxt build --preset vercel-edge",
    "clean": "nuxt cleanup && rm -rf .vercel",
    "dev": "yarn outdated & nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:watch:coverage": "vitest watch --coverage",
    "typecheck": "nuxt typecheck",
    "validate:html": "html-validator .output/public/fr/fabien .output/public/en/fabien --continue --ignore-config=ignore-html.config.txt"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^7.1.1",
    "@nuxtjs/i18n": "^10.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "nuxt": "^4.2.1",
    "nuxt-gtag": "^4.1.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/test-utils": "^3.20.1",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "playwright-core": "^1.56.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "vue-tsc": "^3.1.3",
    "w3c-html-validator": "^2.0.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "version": "0.0.0"
}
