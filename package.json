{
  "name": "cv-with-nuxt",
  "license": "MIT",
  "engines": {
    "node": ">=12.15.0"
  },
  "scripts": {
    "audit:packages": "yarn install --audit",
    "build": "nuxt build",
    "clear": "yarn test --clearCache",
    "dev": "yarn outdated & yarn dev:offline",
    "dev:offline": "nuxt",
    "generate": "nuxt generate --fail-on-error",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "now-build": "yarn generate",
    "start": "nuxt start",
    "start:static": "yarn http-server -p 3000 ./dist",
    "test": "jest --config ./config/jest.config.js",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "validate:lighthouse": "yarn lhci autorun --config=./config/lighthouserc.js",
    "validate:pages": "node scripts/validate_pages.js"
  },
  "dependencies": {
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/tailwindcss": "^3.0.2",
    "nuxt": "^2.14.5",
    "nuxt-i18n": "^6.15.1",
    "nuxt-lazy-load": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@lhci/cli": "^0.5.0",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@types/jest": "^26.0.13",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "html-validator": "^5.1.16",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.3.0",
    "mock-fs": "^4.13.0",
    "pa11y": "^5.3.0",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "vue-jest": "^3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  }
}
